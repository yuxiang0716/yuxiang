#include <bits/stdc++.h>
#include <windows.h> 
using namespace std;

int main() {
    
    int map[12][12];
    int n=1,e,x,y=0;
    int s[11][2];
    int an=5;
    s[0][0]=5;
    s[0][1]=3;
    bool aaa=false;
    bool bbb=false;
    bool ccc=false;
    
    memset(map, 0, sizeof(map)); 
    
	for(int i=1;i<11;i++)
    {
    	for(int j=1;j<11;j++)
    	    {
    	    	map[i][j]=1;
			}
	}
		x=s[0][0];
			y=s[0][1];
			map[x][y]=8;
	
	
		
	cout<<endl<<"welcome to snake game"<<endl;
	cout<<"Rule 1: You must eat 5 point and win"<<endl;
	cout<<"Rule 2: The track you have walked cannot be walked again."<<endl;
	cout<<"Rule 3: Dont hit the border"<<endl;
	cout<<"Rule 4: Have fun"<<endl;
	cout<<"Rule 5: 8 is body||9 is apple||1 is border"<<endl;
	cout<<"Rule 6: ENTER 1 is left||5 is up||2 is down||3 is right"<<endl<<endl;

	cout<<"start"<<endl;	
	
	
	for (int i=0;i<12;i++)
		{
			cout<<endl;
			for(int j=0;j<12;j++)
				{
					cout<<map[i][j];
				}
		}
	
		
while(bbb==false)
{	
		
		
		
		while(ccc!=true)
		{
			srand(time(0));
    		int num1 = rand() % 10 + 1; 
    		int num2 = rand() % 10 + 1;
    		
    		
    		
			for(int i=0;i<n;i++)
			{
				x=s[i][0];
				y=s[i][1];
	
				if(map[num1][num2]!=8 && map[num1][num2]!=0 && map[num1][num2]!=9)
				{
					ccc=true;
					
					map[num1][num2]=9;
					break;
					
				}
				
		
			}
		}
	ccc=false;
	

	
	
	cin>>an;
	
	

		
	if(an==3)//d
		{
			s[0][1]++;
			
		}
	if(an==1)//a
		{
			s[0][1]=s[0][1]-1;
			
		}
	if(an==2)//s
		{
			s[0][0]++;
			
		}
	if(an==5)//w
		{
			s[0][0]=s[0][0]-1;
			
		}
		x=s[0][0];
		y=s[0][1];
		
		if(map[x][y]==9)
		{	 
			cout <<endl<<"great"<<n<<endl;
			n++;
		}
			cout<<endl<<"continue"<<endl;
	if(map[s[0][0]][s[0][1]]==0||map[s[0][0]][s[0][1]]==8)
		{
			cout<<endl<<endl<<endl<<endl<<endl<<endl<<"gg";
			break;
			
		}
		
			map[x][y]=8;
		
	for (int i=0;i<12;i++)
		{
			cout<<endl;
			for(int j=0;j<12;j++)
				{
					cout<<map[i][j];
				}
		}
		
		
	cout <<endl<<" point:"<<n<<endl;
	
		
	if(map[x][y]==9)
		{	 
			cout <<endl<<"great"<<n<<endl;
			n++;
		}
	
	if(n>4)
		{
			cout<<endl<<endl<<endl<<endl<<endl<<endl<<"win";
			
			break;
			
		}
	

	
}


	return 0;
}

